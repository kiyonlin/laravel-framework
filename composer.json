{
  "name": "kiyon/laravel-framework",
  "description": "The Laravel Framework Extensions.",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/kiyonlin/laravel-framework",
  "support": {
    "issues": "https://github.com/kiyonlin/laravel-framework/issues",
    "source": "https://github.com/kiyonlin/laravel-framework"
  },
  "authors": [
    {
      "name": "kiyon",
      "email": "kiyonlin@163.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "spatie/laravel-query-builder": "^1.11",
    "tymon/jwt-auth": "1.0.0-rc.3"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "php-mock/php-mock-mockery": "^1.2",
    "orchestra/testbench": "^3.7"
  },
  "autoload": {
    "files": [
      "src/Foundation/helpers.php",
      "src/Support/helpers.php"
    ],
    "psr-4": {
      "Kiyon\\Laravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "5.7.1-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
